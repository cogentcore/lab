fn ClosestPointPlane(width: f32,length: f32, pt: vec3<f32>) -> vec3<f32> {
	var cp = pt;
	if (width == 0.0) {
		cp.y = f32(0);return cp;
	}
	cp.x = clamp(pt.x, -width, width);
	cp.z = clamp(pt.z, -length, length);return cp;
}

