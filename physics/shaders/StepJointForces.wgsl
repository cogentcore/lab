			f = f+(MulQuatVector(xwPQ, axis)*(JointControl(ji, i32(dof), JointControlForce)));
		}
		for (var dof=0; dof<jAngularN; dof++) {
			var di = i32(jLinearN) + i32(dof);
			var axis = JointAxis(ji, di);
			t = t+(MulQuatVector(xwPQ, axis)*(JointControl(ji, di, JointControlForce)));
		}
	}
	}
	SetJointPForce(ji, Negate3(f));
	SetJointCForce(ji, f);
	SetJointPTorque(ji, Negate3(t+(Cross3(dP, f))));
	SetJointCTorque(ji, t+(Cross3(dC, f)));
	Params[0] = params;
}